{
  "manifest_version": 2,

  "name": "tvSleuth",
  "description": "tvSleuth",
  "version": "1.0",

  "browser_action": {
    "default_icon": "assets/images/app.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "http://*/",
    "contextMenus"
  ],
  "icons": {
    "16": "assets/images/app.png"
  },
  "background": {
    "page": "index.html"
  }
}